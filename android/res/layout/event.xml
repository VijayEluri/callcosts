<?xml version="1.0" encoding="utf-8"?>

<TableLayout android:id="@+id/TableLayout01" android:layout_width="fill_parent" android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android" android:stretchColumns="0">
    <TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content" >
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/name" android:text="(Unknown)" android:padding="3dp">
        </TextView><TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/cost" android:text="12 Ft" android:padding="3dp" android:textStyle="bold"></TextView>
    </TableRow>


    <TableRow android:id="@+id/TableRow02" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Vodafone" android:id="@+id/type" android:padding="3dp">
        </TextView><TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/duration" android:text="12 sec" android:padding="3dp"></TextView>
    </TableRow>


</TableLayout>
